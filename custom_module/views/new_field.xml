<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_form_view" model="ir.ui.view">
        <field name="name">sale.order.form.view</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <!-- Add a new notebook page after the customer signature page -->
                <page string="Vehicle Information">
                    <group>
                        <group>
                            <field name="vehicle_part_number" readonly="0" />
                            <field name="is_add_vehicle_data" invisible="1" />
                        </group>
                        <group>
                            <field name="licence_number" readonly="0" />
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>