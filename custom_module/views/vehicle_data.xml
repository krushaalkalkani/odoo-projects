<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="vehicle_data_form_view" model="ir.ui.view">
        <field name="name">vehicles.data.form.view</field>
        <field name="model">vehicles.data</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="vehicle_part_number" />
                        </group>
                        <group>
                            <field name="licence_number" />
                        </group>
                    </group>
                </sheet>
                <footer>
                    <button string="Save Vehicle Details" type="object" class="oe_highlight"
                        name="button_save_data" />
                    
                </footer>
            </form>
        </field>
    </record>

    <record id="action_vehicle_data" model="ir.actions.act_window">
        <field name="name">Vehicle Details</field>
        <field name="res_model">vehicles.data</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <!-- <record id="action_vehicle_data_return" model="ir.actions.act_window">
        <field name="name">Vehicle Details Return</field>
        <field name="res_model">vehicles.data</field>
        <field name="view_mode">form</field>
        <field name="context">{'default_vehicle_part_number': ,'default_licence_number': ,}</field>
        <field name="target">new</field>
    </record> -->

</odoo>